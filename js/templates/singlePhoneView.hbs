<div class="singleContent">
    <h3>{{itemTitle}}</h3>
    <h4>{{price.min}}$</h4>
    <p>{{ common_descript.value}}</p>
    <a href="#" class="toBasket">В корзину</a> |
    <a href="#" class="compare">Сравнить</a>
</div>

<div class="singleDescr">
    <table>
        <tr><td>{{birthday.title}}</td> <td>{{birthday.value}}</td></tr>
        <tr><td>{{band.title}}</td> <td>{{band.value}}</td></tr>
        <tr><td>{{smart.title}}</td> <td>{{smart.value}}</td></tr>
        <tr><td>{{os.title}}</td> <td>{{os.value}}</td></tr>
        <tr><td>{{cpu.title}}</td> <td>{{cpu.value}}</td></tr>
        <tr><td>{{cpu_freq.title}}</td> <td>{{cpu_freq.value}}</td></tr>
        <tr><td>{{numberofcoresmob.title}}</td> <td>{{numberofcoresmob.value}}</td></tr>
    </table>
</div>